    <div class="form-group">
      <%= f.label :nome %><br>
      <%= f.text_field :nome, :class => 'form-control' %>
    </div>

    <br>

    <div class="form-group">
      <h5> <b> Escolha as imagens do Ã¡lbum </b> </h5>
      <%= f.file_field :imagens, :multiple => true %>
